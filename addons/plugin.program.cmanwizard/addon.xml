<?xml version="1.0" encoding="UTF-8"?>
<addon id="plugin.program.cmanwizard" version="1.0.0" name="CMAN Wizard" provider-name="CMAN">
    <summary>CMAN Wizard for Kodi</summary>
    <description>This addon provides the CMAN Wizard, which includes various builds for Kodi.</description>
    <platform>all</platform>
    <extension point="xbmc.addon.python" library="default.py" />
    <requires>
        <import addon="xbmc.python" version="2.0.0" />
    </requires>
    <resources>
        <icon>icon.png</icon>
        <fanart>fanart.jpg</fanart>
    </resources>
    <dependencies>
        <dependency addon="script.module.cman" version="1.0.0" />
    </dependencies>
</addon>
